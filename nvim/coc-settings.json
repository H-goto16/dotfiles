{
  "coc.preferences.enableLinkedEditing": false,
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.formatOnSave": true,
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.willSaveHandlerTimeout": 1000,
  "coc.source.file.ignoreHidden": false,
  "codeLens.enable": true,
  "codeLens.position": "eol",
  "colors.enable": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.floatConfig": {
    "border": true,
    "rounded": true
  },
  "diagnostic.format": "%message [%source]",
  "diagnostic.virtualText": true,
  "eslint.autoFixOnSave": true,
  "eslint.format.enable": true,
  "eslint.enable": false,
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "snippets.ultisnips.enable": false,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.defaultSortMethod": "none",
  "suggest.detailField": "abbr",
  "suggest.floatConfig": {
    "border": true,
    "rounded": true
  },
  "biome.enable": true,
  "biome.requireConfiguration": true,
  "suggest.selection": "recentlyUsedByPrefix",
  "tsserver.experimental.enableProjectDiagnostics": true,
  "tsserver.implicitProjectConfig.checkJs": true,
  "tsserver.useLocalTsdk": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "typescript.format.semicolons": "ignore",
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.preferences.useAliasesForRenames": false,
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/18.1.3/clangd_18.1.3/bin/clangd"
}
